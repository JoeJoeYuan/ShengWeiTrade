@{
    ViewBag.Title = "Left";
    Layout = null;
}
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>Untitled Page</title>
    <link href="@Url.Content("/Areas/Back/css/admin.css")" rel="stylesheet" type="text/css" />
    <script language="javascript">
        function expand(el) {
            childObj = document.getElementById("child" + el);

            if (childObj.style.display == 'none') {
                childObj.style.display = 'block';
            }
            else {
                childObj.style.display = 'none';
            }
            return;
        }
    </script>

</head>
<body style="background-color: #e3effb; height: 100%; width: 100%;overflow: scroll;text-align:center;" align="center">
    <table height="100%" cellspacing="0" cellpadding="0" width="170" background="@Url.Content("/Areas/Back/images/menu_bg.jpg")"
        border="0">
        @Html.Raw(ViewBag.funtion)
    </table>
</body>
</html>

